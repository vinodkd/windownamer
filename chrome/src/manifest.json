{
  "name"            : "Window Namer",
  "version"         : "0.1",
  "description"     : "Prefix all tabs in a window with a context tag so you can see it in the OS taskbar",
  "background_page" : "wn_bg.html",
  "content_scripts" : [
    {
      "matches" : [ "http://*/*", "https://*/*"],
      "js"      : ["wn_cs_setTitle.js"]
    }
  ],
  "browser_action"  : {
    "default_icon"  : "icon.png",
    "popup"         : "wn_pop_getname.html"
  },
  "permissions"     : [
    "tabs","http://*/*", "https://*/*"
  ]
}